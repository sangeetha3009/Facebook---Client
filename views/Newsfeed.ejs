<html ng-app="myapp">
	<head>
		<title>
			Facebook
		</title>
		<link rel='stylesheet' href='/stylesheets/bootstrap.min' />
		<link rel='stylesheet' href='/stylesheets/style_about.css' />
		<link rel='stylesheet' href='/stylesheets/style_news.css' />
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap.css">
		<link href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css" rel="stylesheet">
		<script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.2.26/angular.js"></script>
		<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.js"></script>
		<script src="http://code.jquery.com/ui/1.9.2/jquery-ui.js"></script>
		<script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.0/jquery.js"></script>
		<script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.2.26/angular-route.js"></script>
		<script src="//netdna.bootstrapcdn.com/twitter-bootstrap/2.2.2/js/bootstrap.js"></script>
		<meta http-equiv="Cache-Control" content="no-cache, no-store, must-revalidate" />
		<meta http-equiv="Pragma" content="no-cache" />
		<meta http-equiv="Expires" content="0" />
	</head>
	<body>
	<div class="wrapper">
	<div class="row">
		<div class="box">
		    
	    	<div class="column" id="main"  ng-controller="searchcnt" ng-init="init()">                
	        	<!-- top nav -->
	             <div class="navbar navbar-blue navbar-static-top">  
	             	<div class="navbar-header">
	                	<button class="navbar-toggle" type="button" data-toggle="collapse" data-target=".navbar-collapse">
	                     <span class="sr-only">Toggle</span>
	                     <span class="icon-bar"></span>
	          			 <span class="icon-bar"></span>
	          			 <span class="icon-bar"></span>
	                     </button>
	                      <a href="/" class="logo">f</a>
	                  	</div>
	                  	<nav class="collapse navbar-collapse" role="navigation">
	                    <form class="navbar-form navbar-left">
						<div class="input-group input-group-sm">
						<input list="browsers" class="form-control" id="search" placeholder="Search Facebook"  name="srch-term" type="text" id="srch-term" ng-model="srch"/>
							<datalist id="browsers">                     
	                                    <option ng-repeat="app in userlist" value={{app}}></option>
	                              
	                        </datalist>
							<div class="input-group-btn">
								<a href=#/about/{{srch}}><button class="btn btn-default"><i class="glyphicon glyphicon-search"></i></button></a>
							</div>
						</div>
						</form>
						<ul class="nav navbar-nav" id="mainButton">
	                      <li>
	                        <a href="#about/" ng-controller="searchcnt" ng-init="initsession()">{{uname}}</a>
	                      </li>
	                      <li>
	                        <a href="#news/">Home</a>
	                      </li>
	                     </ul>
	                  
	                    <ul class="nav navbar-nav" id="logoutButton">
	                 	<form action="logout" method="post">
							<button type="submit" value="Logout">Logout</button>
						</form>
	                    </ul>                   
	                 </nav>
	                </div>
	                	<div ng-view></div>
	                </div>
	                <script
						src="angularjs/app.js">
					</script>                
	                <!-- /top nav -->
	</body>                          
</html>                     
	
